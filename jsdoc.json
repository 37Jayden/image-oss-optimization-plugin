{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
    },
    "source": {
      "include": [
        "lib/index.js"
      ],
      "exclude": [],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "template": "./node_modules/ink-docstrap/template",
      "encoding": "utf8",
      "destination": "./out/",
      "recurse": true,
      "readme": "./history.md"
    }
  }

 